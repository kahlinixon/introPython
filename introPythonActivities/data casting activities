# convert the following datatypes into floats.
number_of_pencils = 12
number_of_bookbags = 100
print(float(number_of_pencils))
print(float(number_of_bookbags))

# convert the following floats into integers.
12.34
23.33
print(int(12.34))
print(int(23.33))

# modify the code below to print an integer number.
num_one = 20
num_two = 23.45

print(int(num_one + num_two))


